{
    "name": "linknacional/whmcsbbpix",
    "version": "1.0.0",
    "type": "project",
    "license": "proprietary",
    "description": "Para pagamentos por Pix pela API do Banco do Brasil",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": false,
        "classmap-authoritative": false,
        "htaccess-protect": false,
        "secure-http": true,
        "vendor-dir": "./vendor",
        "cache-dir": "./composer-cache",
        "data-dir": "./composer-data",
        "archive-dir": "./composer-archive",
        "bin-dir": "./vendor/bin",
        "platform-check": "php-only",
        "platform": {
            "php": "8.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Lkn\\BBPix\\": "src"
        }
    },
    "authors": [
        {
            "name": "Link Nacional",
            "email": "ticket@linknacinal.com"
        }
    ],
    "require": {
        "chillerlan/php-qrcode": "4.3.4"
    }
}
